{
  "parser": "babel-eslint",
  "plugins": ["jest", "react"],
  "extends": ["airbnb-base"],
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "max-len": ["warn", 180]
  }
}
